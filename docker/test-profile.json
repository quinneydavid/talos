{
  "id": "cp1-profile",
  "name": "Talos controlplane Node cp1",
  "boot": {
    "kernel": "/assets/vmlinuz",
    "initrd": [
      "/assets/initramfs.xz"
    ],
    "args": [
      "initrd=initramfs.xz",
      "init_on_alloc=1",
      "init_on_free=1",
      "slab_nomerge",
      "pti=on",
      "console=tty0",
      "console=ttyS0",
      "printk.devkmsg=on",
      "talos.platform=metal",
      "talos.config=http://matchbox.lan:8080/assets/controlplane-cp1.yaml"
    ]
  }
}
